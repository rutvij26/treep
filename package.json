{
  "name": "treep",
  "version": "1.1.0",
  "description": "Lightweight, ultra-fast TypeScript library for graph and tree data structures with comprehensive traversal algorithms",
  "keywords": [
    "graph",
    "tree",
    "data-structure",
    "traversal",
    "bfs",
    "dfs",
    "typescript",
    "hierarchical"
  ],
  "author": {
    "name": "Rutvij Sathe",
    "url": "https://rutvijsathe.dev",
    "github": "https://github.com/rutvij26"
  },
  "contributors": [
    {
      "name": "Rutvij Sathe",
      "url": "https://rutvijsathe.dev",
      "github": "https://github.com/rutvij26"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rutvij26/treep.git"
  },
  "homepage": "https://github.com/rutvij26/treep",
  "bugs": {
    "url": "https://github.com/rutvij26/treep/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc src/index.ts --out docs",
    "docs:serve": "typedoc src/index.ts --out docs --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "size": "size-limit",
    "example:social": "ts-node examples/social-network.ts",
    "example:supply": "ts-node examples/supply-chain.ts",
    "example:json": "ts-node examples/nested-json.ts",
    "prepublishOnly": "npm run test && npm run build && npm run size"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "size-limit": "^11.1.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.0",
    "typescript": "^5.5.3"
  }
}
